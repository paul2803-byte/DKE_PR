@prefix sh:	<http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://www.w3.org/2022/example#> .
@prefix property: <http://wwww.somewhere/property#> .
@prefix type: <http://www.somewhere/type#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

ex:FlightShape a sh:NodeShape ;
	sh:targetClass "http://www.somewhere/type#Flight" ;
	sh:closed true ;
	sh:property [
	    sh:path rdf:type ;
	] ;
	sh:property [
	    sh:path property:Icao24 ;
	    sh:maxCount 1 ;
        sh:minCount 2 ;
        sh:dataType xsd:string ;
        sh:length 3;
    ] ;
    sh:property [
        sh:path property:Country ;
    	sh:maxCount 1 ;
    	sh:dataType xsd:string ;
    ] ;
    sh:property [
        sh:path property:HasState ;
        sh:dataType "http://www.w3.org/2022/example#State" ] .
